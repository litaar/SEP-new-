--database: ./database.db
CREATE TABLE IF NOT EXISTS "QuizQ"(
id INTEGER PRIMARY KEY AUTOINCREMENT,
name TEXT NOT NULL, 
type TEXT NOT NULL,
answer TEXT NOT NULL,
category TEXT
    
);



